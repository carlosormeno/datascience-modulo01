{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-upbk-cos-devbk-p8v"
		},
		"FS_LS_Banco01_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FS_LS_Banco01'"
		},
		"ADLS_LinkedService_Banco_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LinkedService_Banco'"
		},
		"FS_LS_Banco01_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\LenovoRed\\C$\\trabajo"
		},
		"FS_LS_Banco01_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "MicrosoftAccount\\carlos.ormeno@outlook.es"
		},
		"ADLS_LinkedService_Banco_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsupbkcosdevbkk6k.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-upbk-cos-devbk-p8v_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LinkedService_Banco_accountKey": {
						"value": "[parameters('ADLS_LinkedService_Banco_accountKey')]"
					},
					"FS_LS_Banco01_password": {
						"value": "[parameters('FS_LS_Banco01_password')]"
					},
					"ADLS_LinkedService_Banco_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LinkedService_Banco_properties_typeProperties_url')]"
					},
					"FS_LS_Banco01_properties_typeProperties_host": {
						"value": "[parameters('FS_LS_Banco01_properties_typeProperties_host')]"
					},
					"FS_LS_Banco01_properties_typeProperties_userId": {
